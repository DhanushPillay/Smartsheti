{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "frontend/**",
            "use": "@vercel/static"
        },
        {
            "src": "data/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/backend/prices.json",
            "dest": "/backend/prices.json"
        },
        {
            "src": "/frontend/(.*)",
            "dest": "/frontend/$1"
        },
        {
            "src": "/data/(.*)",
            "dest": "/data/$1"
        },
        {
            "src": "/(.*)\\.html",
            "dest": "/$1.html"
        },
        {
            "src": "/(.*)\\.css",
            "dest": "/$1.css"
        },
        {
            "src": "/(.*)\\.js",
            "dest": "/$1.js"
        },
        {
            "src": "/(.*)\\.json",
            "dest": "/$1.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}